            <string>//set bullet type(type 1 = phyiscal, type 2 = magic)
type = 1;
hasHit = 0;
//bullet speed defintions
/*if(Player.facing_Down){
    hspBullet = Player.hsp;
    vspBullet = Player.vsp + BulletSpeed;
}else if(Player.facing_Up){
    hspBullet = Player.hsp;
    vspBullet = Player.vsp - BulletSpeed;
}else */if(Player.facing_Right){
    hspBullet = Player.hsp + BulletSpeed;
    vspBullet = Player.vsp;
}else if(Player.facing_Left){
    hspBullet = Player.hsp - BulletSpeed;
    vspBullet = Player.vsp;    
}
// minimuze speed to 10% of base
if (Player.facing_Right &amp;&amp; Player.facing_Down = 0 &amp;&amp; Player.facing_Up = 0){
    if (hspBullet &lt; BulletSpeed / 10){
        hspBullet = BulletSpeed / 10;
    }
}
if (Player.facing_Left &amp;&amp; Player.facing_Down = 0 &amp;&amp; Player.facing_Up = 0){
    if (abs(hspBullet) &lt; BulletSpeed / 10){
        hspBullet = -BulletSpeed / 10;
    }
}

if (Player.facing_Down){
    if (vspBullet &lt; BulletSpeed / 10){
        vspBullet = BulletSpeed / 10;
    }
}

if (Player.facing_Up){
    if (abs(vspBullet) &lt; BulletSpeed / 10){
        vspBullet = -BulletSpeed / 10;
    }
}
// check direction
if ((Player.facing_Up || Player.facing_Down) &amp;&amp; Player.vsp = 0){
    firedV = 1;
    firedH = 0;
    firedD = 0;
}else if((!Player.facing_Up || !Player.facing_Down) &amp;&amp; Player.vsp = 0){
    firedV = 0;
    firedH = 1;
    firedD = 0;
}else{
    firedV = 0;
    firedH = 0;
    firedD = 1;
}
//damage of bullet
damage = BulletDmgX * (Player.CurAttack)
